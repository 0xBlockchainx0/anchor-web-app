{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:1337/",
  "scripts": {
    "start": "multiplerun start",
    "start:chrome": "multiplerun start:chrome",
    "build": "CI=true react-scripts build",
    "test": "react-scripts test --colors",
    "coverage": "react-scripts test --colors --coverage",
    "eject": "react-scripts eject",
    "pack": "rocket-punch build",
    "publish": "rocket-punch publish",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "multiplerun": {
    "start": [
      "PORT=9433 BROWSER=none npx react-scripts start",
      "PORT=9433 TS_NODE_PROJECT=tsconfig.script.json node -r ts-node/register -r tsconfig-paths/register scripts/start.ts"
    ],
    "start:chrome": [
      "PORT=9433 BROWSER=none npx react-scripts start",
      "open http://localhost:9433"
    ]
  },
  "eslintConfig": {
    "extends": [
      "../.eslintrc.js"
    ]
  },
  "browserslist": {
    "production": [
      "chrome > 60",
      "firefox > 60",
      "safari > 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-storysource": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/node-logger": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@terra-money/terra.js": "^1.2.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/autoprefixer": "^9.7.2",
    "@types/color": "^3.0.1",
    "@types/eslint": "^6.8.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/postcss-nested": "^4.2.3",
    "@types/puppeteer": "^5.4.2",
    "@types/ramda": "^0.27.32",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.5",
    "@types/testing-library__jest-dom": "^5.9.5",
    "bignumber.js": "^9.0.1",
    "color": "^3.1.3",
    "downshift": "^6.0.7",
    "fastdom": "^1.0.9",
    "graphql": "^15.4.0",
    "multiplerun": "^1.1.2",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "rocket-punch": "^2.3.2",
    "styled-components": "^5.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "use-resize-observer": "^7.0.0",
    "web-vitals": "^0.2.4"
  }
}
